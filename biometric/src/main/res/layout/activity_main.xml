<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BiometricActivity">

  <com.google.android.material.button.MaterialButton
      android:id="@+id/open_link"
      android:layout_width="0dp"
      android:layout_height="80dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      android:text="OPEN LINK"
      app:layout_constraintStart_toStartOf="parent"/>

  <Button
      android:id="@+id/open_link_menu_items"
      android:layout_width="0dp"
      android:layout_height="80dp"
      android:text="OPEN LINK MENU ITEMS"
      app:layout_constraintBottom_toTopOf="@id/open_link"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"/>

  <Button
      android:id="@+id/customtab"
      android:layout_width="0dp"
      android:layout_height="80dp"
      android:text="customtab"
      app:layout_constraintBottom_toTopOf="@id/open_link_menu_items"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"/>

  <Button
      android:id="@+id/customtab_session"
      android:layout_width="0dp"
      android:layout_height="80dp"
      android:text="customtab session"
      app:layout_constraintBottom_toTopOf="@id/customtab"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
